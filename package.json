{
  "name": "nuxtjs",
  "version": "1.0.0",
  "description": "Mazhin",
  "author": "Tomer Lev",
  "private": true,
  "scripts": {
    "dev-api": "NODE_ENV=dev vercel dev --listen 3001",
    "dev-nuxt": "BASE_API_URL=http://localhost:3001 nuxt",
    "dev": "concurrently \"npm:dev-api\" \"npm:dev-nuxt\"",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "now-build": "if test \"$NODE_ENV\" != \"dev\" ; then nuxt generate ; fi"
  },
  "dependencies": {
    "@nuxt/ufo": "^0.5.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bootstrap-vue": "^2.0.4",
    "@nuxtjs/pwa": "3.3.5",
    "axios": "^0.21.1",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.21.2",
    "cookie-parser": "^1.4.5",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "express": "^4.17.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "nuxt": "^2.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "sirv": "^1.0.0",
    "vue-form-wizard": "^0.8.4",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
